{
  "$schema": "https://raw.githubusercontent.com/AngelMunoz/Perla/v1.0.0-beta-011/perla.schema.json",
  "index": "./index.html",
  "fable": {
    "project": "./src/DevApp.fsproj",
    // output extension of the fable files
    "extension": ".fs.js",
    // enable Fable source map output
    "sourceMaps": true,
    // where to output these files
    "outDir": "./build"
  },
  "mountDirectories": {
    "/": "./build",
    "/assets": "./assets"
  }
}